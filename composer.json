{
	"name" : "n2n/valobj",
	"type" : "library",
	"description" : "Contains n2n typical implementations of common value objects.",
	"keywords" : [
		"n2n",
		"batch job"
	],
	"homepage" : "https://n2n.rocks/",
	"license" : "LGPL-3.0-or-later",
	"authors" : [
        {
			"name" : "Andreas von Burg",
			"email" : "a@von-burg.net",
			"homepage" : "https://www.von-burg.net/"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable": true,
	"config" : {
		"vendor-dir" : "src/vendor"
	},
	"require" : {
        "n2n/n2n-spec-valobj": "^1",
        "n2n/n2n-bind": "~7.4"
    },
	"autoload" : {
		"psr-4" : {
			"valobj\\" : "src/app/valobj"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"valobj\\": "src/test/valobj"
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.6"
	}
}